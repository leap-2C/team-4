datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  email     String   @unique
  name      String?
  role      Role     @default(USER)
  password  String 
}
model bankCard{
  id String @id @default(cuid())
  country String 
  firstName String 
  lastName String 
  timeStamp DateTime @default(now())
  expriryDate Int 
  userId String
}


enum Role {
  USER
  ADMIN
}